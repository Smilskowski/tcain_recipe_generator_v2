{
  "name": "bag_of_crafting",
  "version": "1.0.0",
  "description": "Tainted Cain Recipe Generator for The Binding of Isaac: Repentance",
  "main": "dist/index.js",
  "scripts": {
    "build": "xcopy /E /I /Y /d assets dist\\assets 2>nul & tsc -p .",
    "build:clean": "rimraf dist && npm run build",
    "selftest": "npm run build && node dist/selftest.js",
    "start": "npm run build && node dist/index.js",
    "dev": "tsc -w",
    "test": "npm run selftest",
    "lint": "tslint -p .",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "isaac",
    "binding-of-isaac",
    "tainted-cain",
    "bag-of-crafting",
    "recipe-generator",
    "repentance"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.6",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "xml2js": "^0.4.23"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/tcain_recipe_generator_v2.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/tcain_recipe_generator_v2/issues"
  },
  "homepage": "https://github.com/your-username/tcain_recipe_generator_v2#readme"
}
